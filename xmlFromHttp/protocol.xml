					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type width="110">button</Type>
				<Discreets>
					<Discreet>
						<Display>Send Request</Display>
						<Value>Send Request</Value>
					</Discreet>
				</Discreets>
			</Measurement>
		</Param>
		
	</Params>

	<QActions>
		<QAction id="1" name="Precompiled Code" encoding="csharp" options="precompile" />
		<QAction id="2" name="After Startup" encoding="csharp" triggers="2" />
	</QActions>

	<HTTP>
		<Session id="1" name="Prova">
			<Connection id="1" name="Prova">
				<Request verb="GET" url="testDataminer.xml"></Request>
				<Response statusCode="5">
					<Content pid="6"></Content>
				</Response>
			</Connection>
		</Session>
	</HTTP>


	<Groups>
		<Group id="1">
			<Name>Prova</Name>
			<Description>Prova</Description>
			<Type>poll</Type>
			<Content>
				<Session>1</Session>
			</Content>
		</Group>
		<Group id="2">
			<Name>After Startup</Name>
			<Description>After Startup</Description>
			<Type>poll action</Type>
			<Content>
				<Action>2</Action>
			</Content>
		</Group>
	</Groups>

	<Triggers>
		<Trigger id="1">
			<Name>After Startup</Name>
			<On>protocol</On>
			<Time>after startup</Time>
			<Type>action</Type>
			<Content>
