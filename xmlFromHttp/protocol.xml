			<Interval>75</Interval>
			<Content>
			</Content>
		</Timer>
		<Timer id="3">
			<Name>Slow Timer (1h)</Name>
			<Time initial="true">3600000</Time>
			<Interval>75</Interval>
			<Content>
			</Content>
		</Timer>
	</Timers>

