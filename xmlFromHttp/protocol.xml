				</Positions>
			</Display>
			<Measurement>
				<Type>string</Type>
			</Measurement>
		</Param>
		<Param id="6" trending="false">
			<Name>responseContent</Name>
			<Description>Response Content</Description>
			<Type>read</Type>
			<Information>
				<Subtext>Response Content.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
				<Positions>
					<Position>
						<Page>General</Page>
						<Column>0</Column>
						<Row>1</Row>
					</Position>
				</Positions>
			</Display>
			<Measurement>
				<Type lines="10">string</Type>
			</Measurement>
		</Param>
		<Param id="54">
			<Name>SendRequest</Name>
			<Type>write</Type>
			<Information>
				<Subtext>Manually send out the request.</Subtext>
			</Information>
			<Interprete>
				<RawType>other</RawType>
				<Type>string</Type>
				<LengthType>next param</LengthType>
			</Interprete>
			<Display>
				<RTDisplay>true</RTDisplay>
